{
    "name": "ichhabrecht/t3tags",
    "description": "Generate tag fields for every record type",
    "type": "typo3-cms-extension",
    "keywords": [
        "TYPO3 CMS",
        "Extension",
        "Tags"
    ],
    "homepage": "https://github.com/IchHabRecht/t3tags",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "Nicole Cordes",
            "email": "typo3@cordes.co",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">= 7.0, < 7.4",
        "typo3/cms-core": "^8.7"
    },
    "require-dev": {
        "nimut/testing-framework": "4.x-dev"
    },
    "replace": {
        "typo3-ter/t3tags": "self.version"
    },
    "config": {
        "vendor-dir": ".Build/vendor",
        "bin-dir": ".Build/bin"
    },
    "extra": {
        "typo3/cms": {
            "extension-key": "t3tags",
            "cms-package-dir": "{$vendor-dir}/typo3/cms",
            "app-dir": ".Build",
            "web-dir": ".Build/public"
        }
    }
}
